{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ZBusinessPartner-PartnerModule",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/zbusinesspartner-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsPartnerModule/index.html",
        "--ui5",
        "{\"version\":\"1.78.14\"}"
      ],
      "cwd": "${workspaceFolder}/PartnerModule",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/ZBusinessPartner/PartnerModule\"}"
      },
      "envFile": "${workspaceFolder}/PartnerModule/.env1",
      "preLaunchTask": "Build ZBusinessPartner"
    }
  ]
}